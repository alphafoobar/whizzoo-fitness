<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
  <script src="js/jquery-1.3.2.min.js"></script>  
  <script>
  var timer = new Date().getTime();
  $(document).ready(function(){
    $("<b>hello </b>").appendTo("#images").append("world!<br/>");
    $.getJSON(/*"js/weather.js", */"http://ws.geonames.org/weatherIcaoJSON?ICAO=NZWN&callback=?",
        function(data){
    $("<b>hello </b>").appendTo("#images").append("world!<br/>");
        	$("<br/><span>stationName: </span>").appendTo("#images").append(data.weatherObservation.stationName);
        	$("<br/><span>clouds: </span>").appendTo("#images").append(data.weatherObservation.clouds);
        	$("<br/><span>windDirection: </span>").appendTo("#images").append(data.weatherObservation.windDirection);
        	$("<br/><span>temperature: </span>").appendTo("#images").append(data.weatherObservation.temperature);
        	$("<br/><span>windSpeed: </span>").appendTo("#images").append(data.weatherObservation.windSpeed);
        	$("<br/><span>humidity: </span>").appendTo("#images").append(data.weatherObservation.humidity);
        	$("<br/><span>datetime: </span>").appendTo("#images").append(data.weatherObservation.datetime);
        	$("<br/><span>download time: </span>").appendTo("#images").append((new Date().getTime()-timer)+"ms");
        });
  });
  </script>
  <style>img{ height: 100px; float: left; border: 0px;}</style>
</head>
<body>
  <div id="images"></div>
</body>
</html>