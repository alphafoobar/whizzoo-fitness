@CHARSET "utf-8";
body {
	font-family:'Franklin Gothic Medium'; font-size: 12px; margin: 0px;
}

#banner_shadow {
	background-color: #484537;
}

#banner {
	background-color: #f4d7d7;
}

.header {
	color: #28170b; padding: 10px;
}

.tabs {
	background-color: #484537; 
	color: #e3e2db; 
	margin: 4px; 
	padding: 6px;
}

.tabs a {
	color: #e3e2db; 
	text-decoration: none;
}

.tabs a:hover {
	color: #e3e2db; 
	text-decoration: underline;
}
